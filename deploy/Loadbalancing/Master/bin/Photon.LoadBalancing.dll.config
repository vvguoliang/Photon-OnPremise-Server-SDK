      <setting name="LimitGameList" serializeAs="String">
        <value>500</value>
      </setting>
      <setting name="LimitGameListUpdate" serializeAs="String">
        <value>500</value>
      </setting>
      <setting name="LimitSqlFilterResults" serializeAs="String">
